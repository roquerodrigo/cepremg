
    {% for date,values in dados %}
        <tr class="row">
            <td class="col-md-1">{{date}}</td>
            {% for val in values %}
                <td class = "col-md-1">
                    <div class="progress">
                        <div class="progress-bar-success" role="progressbar" aria-valuenow="{{val * 100}}" aria-valuemin="0" aria-valuemax="100" style="{{ val * 100 }}">{{val * 100}}% </div>
                    </div>
                </td>
            {% endfor %}
        </tr>
    {% endfor %}